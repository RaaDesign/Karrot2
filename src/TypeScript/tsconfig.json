{
    "compilerOptions": {
        "target": "es5",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "out": "../../Karrot2.js"
    },
    "filesGlob": [
        "**/**/*.ts"
    ],
    "compileOnSave": true,
    "buildOnSave": true,
    "files": [
        "_forms.ts",
        "_navbar.ts",
        "Karrot2.ts",
        "ts_libs/jquery.d.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
